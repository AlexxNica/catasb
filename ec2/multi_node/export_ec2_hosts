
CMD="./parse_yaml.py -f ec2_hosts.yml"

MASTERS="$(${CMD} -m)"          # list of masters
NODES="$(${CMD} -n)"            # list of nodes
WILDCARD_DNS_HOST="$(${CMD} -w)"     # wildcard DNS host

# set EC2_HOSTS which will be appended to the EXTRA_VARS
export EC2_HOSTS=\
", \
\"masters\":${MASTERS}, \
\"nodes\":${NODES}, \
\"wildcard_dns_host\":${WILDCARD_DNS_HOST}\
"
